{
    "root": true,
    "extends": [
        "eslint:recommended",
        "plugin:node/recommended"
    ],
    "env": {
        "es6": true,
        "jest": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "strict": ["error", "global"]
    },
    "ignorePatterns": ["test/**/test.js"]
}
